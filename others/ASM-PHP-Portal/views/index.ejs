<body ng-app="AsmPortal" ng-cloak layout="row" ng-controller="MainCtrl" layout-fill  >
  <div flex layout="column">
    <div>
    <md-toolbar  class="md-hue-2" layout-align="center" >
      <div class="md-toolbar-tools" layout="row" >
        <img src="/images/logo.svg">
        <h2>ASM Delegation Demo Portal</h2>
      </div>
    </md-toolbar>
  </div>
    <!--en toolbar-->
  <div flex style="background-color:white;">
    <md-content layout="row" >
      <!-- fabspeeddial-->
      <div flex="10" style="background-color:white;">
        <md-fab-speed-dial md-direction="down" class="md-scale" md-open="menufabisOpen"
        ng-mouseenter="menufabisOpen=true" ng-mouseleave="menufabisOpen=false">
          <md-fab-trigger>
            <md-button aria-label="Menu" class="md-fab md-warn">
              <md-tooltip md-direction="right">Menu</md-tooltip>
                <i class="material-icons" >&#xE5D2;</i>
            </md-button>
          </md-fab-trigger>
          <md-fab-actions>
              <md-button aria-label="Edit Black Lists" class="md-fab md-raised md-mini" hui-sref="policies" href="/#/policies">
                <md-tooltip md-direction="right">Edit Black Lists</md-tooltip>
                <i class="material-icons myfab" >&#xE051;</i>
              </md-button>
              <md-button aria-label="ASM Management" class="md-fab md-raised md-mini" href="/#/asmmgt" >
                <md-tooltip md-direction="right">ASM Management</md-tooltip>
                <i class="material-icons myfab" >&#xE869;</i>
              </md-button>
          </md-fab-actions>
        </md-fab-speed-dial>
      </div>
      <!-- end fabspeeddial-->
      <md-content flex id="content" layout="row"   layout-align="left top">
          <ui-view ></ui-view> <!--here we insert angular template views-->
      </md-content>
      <!--end md content-->
    </md-content>
  </div>
    <!-- end div class container -->
  </div>
   <!-- Angular Material requires Angular.js Libraries -->
  <script src="./javascripts/angular.min.js"></script>
  <script src="./javascripts/angular-animate.min.js"></script>
  <script src="./javascripts/angular-aria.js"></script>
  <script src="./javascripts/angular-messages.js"></script>
  <script src="./javascripts/angular-material.min.js"></script>
